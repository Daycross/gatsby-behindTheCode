    "gatsby-plugin-page-creator": "^4.14.0",
    "gatsby-plugin-react-helmet": "^5.14.0",
    "gatsby-plugin-sitemap": "^5.14.0",
    "gatsby-plugin-styled-components": "^5.14.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "styled-components": "^5.3.5"
